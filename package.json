    "eslint": "^7.12.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-unicorn": "23.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-plugin-xss": "^0.1.10",
    "typescript": "^4.0.5",
    "vue-template-compiler": "^2.6.12"
